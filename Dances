    if character then
        Anim = Animator.new(character, 81312689882073)
        Anim:Play()
        Anim.Stopped:Connect(function()
            Anim:Play()
        end)
             
        writefile("The Hero.mp3", game:HttpGet("https://github.com/lol9182828/Woozynate/raw/refs/heads/main/GO%20DO%20IT%20%23jerseyclub%20%20-%20@ziahfyah%20@-Sj-.mp3"))     
        sound = Instance.new("Sound")
        sound.SoundId = getcustomasset("The Hero.mp3")
        sound.Volume = 2
        sound.Looped = true
        sound.Parent = game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart")
        sound:Play()
    end
end)

